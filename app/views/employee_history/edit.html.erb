<h1><%=getLongName('table_name','employee_histories')%>編集</h1>
<p>社員名: <%=h @employee.employee_name %></p>

<%= form_tag ({:action => 'update', :id => @employee_history} , {:multipart => true}) do %>
  <%= render :partial => 'form' %>
  <%= submit_tag '更新' %>
<% end %>

<%= link_to '表示', :action => 'show', :id => @employee_history, :employee_id => @employee %> |
<%= link_to '一覧に戻る', :action => 'list', :employee_id => @employee %>
